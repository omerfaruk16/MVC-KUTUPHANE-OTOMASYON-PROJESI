    <link href="~/webregister/css/style.css" type="text/css" rel="stylesheet" media="all">
    <!-- //css files -->

    <link rel="stylesheet" href="~/webregister/css/font-awesome.css"> <!-- Font-Awesome-Icons-CSS -->

</head>
<!-- body starts -->
<body>
    <!-- section -->
    <section class="register">
        <div class="register-full">
            <div class="register-left">
                <div class="register">
                    <div class="logo">
                        <a href="#"><span class="fa fa-book" aria-hidden="true"></span></a>
                    </div>
                    <h1>MVC 5 KÜTÜPHANE KAYIT FORMU</h1>
                    <p>
                        Kelimeler olmasaydı, yazı olmasaydı, kitaplar olmasaydı; tarih olmazdı, insanlık kavramı olmazdı.
                        ~HERMAN HESSE~
                    </p>
                    <div class="content3">
                        <ul>
                            <li><a class="" href="~/Login/GirisYap/">GİRİŞ YAP</a></li>
                            <li><a class="read" href="/Vitrin/Index/">SİTEYE DÖN</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="register-right">
                <div class="register-in">
                    <h2 style="text-align:center"><span class="fa fa-pencil"></span> KAYIT FORMUNA HOŞGELDİNİZ</h2>
                    <div class="register-form">
                        <form action="#" method="post">
                            <div class="fields-grid">
                                <div class="styled-input agile-styled-input-top">
                                    <input type="text" name="First name" required="">
                                    <label>ADINIZI GİRİNİZ</label>
                                    <span></span>
                                </div>
                                <div class="styled-input agile-styled-input-top">
                                    <input type="text" name="Second name" required="">
                                    <label>SOYADINIZI GİRİNİZ</label>
                                    <span></span>
                                </div>
                                <div class="styled-input">
                                    <input type="email" name="Email" required="">
                                    <label>EMAIL ADRESİNİZ</label>
                                    <span></span>
                                </div>
                                <div class="styled-input">
                                    <input type="tel" name="Phone" required="">
                                    <label>TELEFON NUMARANIZ</label>
                                    <span></span>
                                </div>
                                <div class="styled-input">
                                    <input type="password" name="password" required="">
                                    <label>ŞİFRE</label>
                                    <span></span>
                                </div>
                                <div class="styled-input">
                                    <input type="text" name="username" required="">
                                    <label>KULLLANICI ADI</label>
                                    <span></span>
                                </div>


                                <div class="styled-input agile-styled-input-top">
                                    <select id="category2" required="">
                                        <option value="">OKULUNUZU SEÇİN*</option>
                                        <option value="">KÜTAHYA DUMLUPINAR ÜNİVERSİTESİ</option>
                                        <option value="">AKDENİZ  ÜNİVERSİTESİ</option>
                                        <option value="">SÜLEYMAN DEMİREL ÜNİVERSİTESİ</option>
                                        <option value="">İSTANBUL ÜNİVERSİTESİ</option>
                                        <option value="">İNÖNÜ ÜNİVERSİTESİ</option>
                                        <option value="">BOĞAZİÇİ ÜNİVERSİTESİ</option>
                                        <option value="">NAMIK KEMAL ÜNİVERSİTESİ</option>
                                        <option value="">ATATÜRK ÜNİVERSİTESİ</option>
                                        <option value="">ULUDAĞ ÜNİVERSİTESİ</option>
                                        <option value="">ANADOLU ÜNİVERSİTESİ</option>
                                        <option value="">SELÇUK ÜNİVERSİTESİ</option>
                                        <option value="">SITKI KOÇMAN ÜNİVERSİTESİ</option>
                                        <option value="">KOCAELİ ÜNİVERSİTESİ</option>
                                        <option value="">ANKARA ÜNİVERSİTESİ</option>
                                        <option value="">BALIKESİR ÜNİVERSİTESİ</option>

                                    </select>
                                    <span></span>
                                </div>
                                <div class="clear"> </div>
                                <label class="checkbox"><input type="checkbox" name="checkbox" checked><i></i>Kullanım Sözleşmesini Kabul Etmelisiniz. <a href="#"></a>KABUL EDİYORUM</label>
                            </div>
                            <input type="submit" value="KAYIT OL">
                        </form>
                    </div>
                </div>
                <div class="clear"> </div>
            </div>
            <div class="clear"> </div>
        </div>
        <!-- copyright -->
        <p class="agile-copyright">© @DateTime.Now.Year MVC 5 KULLANICI KAYIT FORMU TÜM HAKLARI SAKLIDIR. | <a href="https://w3layouts.com/" target="_blank"></a></p>
        <!-- //copyright -->
    </section>
    <!-- //section -->
</body>
<!-- //body ends -->
</html>